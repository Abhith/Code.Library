{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.sln"],
          "exclude": ["**/bin/**", "**/obj/**"],
          "src": "src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": ["articles/**/*.md", "*.md", "toc.yml", "restapi/**"]
      }
    ],
    "resource": [
      {
        "files": ["articles/images/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Code.Library",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "template": ["default", "_exported_templates/default"]
  }
}
